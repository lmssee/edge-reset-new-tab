{
  "manifest_version": 3,
  "name": "reset new tab",
  "short_name": "rnt",
  "version": "0.0.0.1",
  "description": " a tolls for reset edge new tab 一个重定向 edge 新选项的插件",
  "icons": {
    "16": "./icons/ico@16x16.png",
    "32": "./icons/ico@32x32.png",
    "48": "./icons/ico@48x48.png",
    "128": "./icons/ico@128x128.png"
  },
  "action": {
    "default_popup": "/popup/index.html",
    "default_icon": {
      "16": "./icons/ico@16x16.png",
      "32": "./icons/ico@32x32.png",
      "48": "./icons/ico@48x48.png",
      "128": "./icons/ico@128x128.png"
    }
  },
  "author": "lmssee",
  "homepage_url": "https://lmssee.com",
  "chrome_url_overrides": {
    "newtab": "/newtab/index.html",
    "default_icon": {
      "16": "./icons/ico@16x16.png",
      "32": "./icons/ico@32x32.png",
      "48": "./icons/ico@48x48.png",
      "128": "./icons/ico@128x128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/contents.js"
      ]
    }
  ],
  "key": "",
  "background": {
    "service_worker": "./background.js"
  },
  "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
    "tabs"
  ]
}