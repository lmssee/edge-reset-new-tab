{
  "name": "reset new tab",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "description": " a tolls for reset edge new tab 一个重定向 edge 新选项的插件",
  "icons": {
    "16": "src/icons/nasaPod16x16.png",
    "32": "src/icons/nasaPod32x32.png",
    "48": "src/icons/nasaPod48x48.png",
    "128": "src/icons/nasaPod128x128.png"
  },
  "action": {
    "default_popup": "src/html/popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "src/html/newTab.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/js/contents.js"
      ]
    }
  ]
}