{
  "type": "module",
  "version": "0.0.4",
  "name": "reset-new-tab",
  "description": "重置 edge 新标签页源码",
  "scripts": {
    "build": "npm run build-popup ",
    "build-popup": "rollup --config rollup.config.popup.js && npm run build-newtab ",
    "build-newtab": "rollup --config rollup.config.newtab.js && npm run build-content",
    "build-content": "rollup --config rollup.config.content.js && npm run build-background ",
    "build-background": "rollup --config rollup.config.background.js",
    "clear": "npx ixxx rm dist",
    "clean": "npx ixxx pkg -u rm node_modules && npm install && npm run build",
    "prettier": "npx prettier . --write",
    "beautify": "npm run prettier",
    "eslint": "npx eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmssee/resetNewTab.git"
  },
  "author": "lmssee <lmssee@outlook.com>  (https://lmssee.com)",
  "license": "ISC",
  "keywords": [
    "reset-new-tab"
  ],
  "homepage": "https://lmssee.github.io/resetNewTab",
  "bugs": {
    "url": "https://github.com/lmssee/resetNewTab/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.11",
    "a-edge-extends-types": "file:../../npm/edge-extends-types/dist",
    "eslint": "^8.57.0",
    "globals": "^15.8.0",
    "javascript-obfuscator": "^4.1.1",
    "prettier": "3.3.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-obfuscator": "^1.1.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1"
  },
  "dependencies": {
    "a-js-tools": "^0.0.0"
  }
}